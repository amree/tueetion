.breadcrumb-container
  %ol.breadcrumb
    %li= link_to "Home", root_path
    %li= link_to "Quantity Fees", quantity_fees_path
    %li.active= @quantity_fee.id

= render 'layouts/flash_messages'

.section-title
  %h2 Quantity Fee

%dl.dl-horizontal
  %dt
    Subject
  %dd
    = @quantity_fee.subject.name
  %dt
    Price
  %dd
    = number_with_precision(@quantity_fee.price, precision: 2)
  %dt
    &nbsp;
  %dd
    = @quantity_fee.is_active
  %dt
    Created at:
  %dd
    = l @quantity_fee.created_at.in_time_zone("Kuala Lumpur")
  %dt
    Updated at:
  %dd
    = l @quantity_fee.updated_at.in_time_zone("Kuala Lumpur")
